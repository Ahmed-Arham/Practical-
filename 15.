#include <stdio.h>
struct Student {
    char name[50];
    int roll;
    float marks;
};
int main() {
    struct Student s;
    printf("Enter name, roll number, and marks: ");
    scanf("%s %d %f", s.name, &s.roll, &s.marks);
    printf("Student Details:\n");
    printf("Name: %s\nRoll: %d\nMarks: %.2f\n", s.name, s.roll, s.marks);
    return 0;
}
